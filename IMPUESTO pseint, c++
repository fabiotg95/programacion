int main()
{
	float sueldo,total,afp,ars,imprent
	cout <<"digite su sueldo:"
	cin >> sueldo;
	cout <<"su sueldo es de:", sueldo
  cout<<"su impuesto de renta es de:", imprent
  cout<<"el dineor que le queda es:",total
  cout<<"su sueldo es de:", sueldo
  cout<<"su impuesto aft es de:", aft
  cout<<"su impuesto ARS es", ARS
  cout<<"el dinero que le queda es", total
	getch();
}


